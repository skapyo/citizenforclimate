 <% if (!(villagesBenevoles!== undefined && villagesBenevoles.length==0)){ %>
<div id="villagesBenevoles" class="bordureGrise">
	<%=translation.PROFIL_LIST_WALK_BENEVOLE%>
<p>
      <table id="listeVillageBenevoles" class="tableau">
	      <thead>
	      <tr>
	          <td><%=translation.PROFIL_LIST_WALK_NAME%></td>
	          <td><%=translation.PROFIL_LIST_WALK_NUMBER_BENEVOLE%></td>
	          <td><%=translation.PROFIL_LIST_WALK_ACCESS%></td>
	      </tr>
	      </thead>
	      <tbody>
	          <%
	          if (villagesBenevoles!== undefined){
	          villagesBenevoles.forEach(function(village) { %>
	        <tr id="<%= village._id %>">
	          <td><%= village.nom %></td>
	          <td><%= village._benevoles.length %></td>
	          <td><a href="/village?villageId=<%= village._id %>"> <button><%=translation.PROFIL_LIST_WALK_ENTER%></button></a></td>
	        </tr>
	          <% });
	          }%>
	      </tbody>
	  </table>
  </div>
<% }%>