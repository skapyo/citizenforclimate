<!-- popin -->
<div class="modal fade" id="popinSupression" tabindex="-1" role="dialog" aria-labelledby="popinLabel" aria-hidden="true">

  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="popinLabel"><%=translation.ACTIVITY_POPIN_DELETE_CONFIRM%></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%=translation.ACTIVITY_POPIN_DELETE_CONFIRM_BODY%>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal"><%=translation.ACTIVITY_POPIN_DELETE_CONFIRM_CANCEL%></button>
        <button type="button" class="btn btn-primary" id="supprimer"><%=translation.ACTIVITY_POPIN_DELETE_CONFIRM_BUTTON%></button>
      </div>
    </div>
  </div>
</div>
<input type="hidden" id="activiteId" />
	
</div>

